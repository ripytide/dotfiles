{
  "OnPropertyChange": {
    "prefix": "opc",
    "body": [
      "private ${1:Type} ${2:_property};",
      "public ${1:Type} ${3:Property}",
      "{",
      "    get => ${2:_property};",
      "    set {",
      "        SetProperty(ref ${2:_property}, value);",
      "    }",
      "}",
      "$0"
    ]
  },
  "DesignerProperty": {
    "prefix": "dp",
    "body": [
      "public static string ${1:Name} {",
      "    get {",
      "        return ResourceManager.GetString(\"${1:Name}\", resourceCulture);",
      "    }",
      "}",
      "$0"
    ]
  }
}
